{
  "name": "utopia-target-finder",
  "version": "1.0.0",
  "description": "Uses public data feed from Utopia to provide province and kingdom information",
  "repository": {
    "type": "git",
    "url": "git@github.com/RJSchmertz/utopia-target-finder"
  },
  "author": "RJ Schmertz",
  "scripts": {
    "webpack": "set \"NODE_ENV=development\" && webpack --progress --colors",
    "webpack-d": "set \"NODE_ENV=development\" && webpack -d --progress --colors --watch",
    "webpack-p": "set \"NODE_ENV=production\" && webpack -p --progress --colors",
    "karma": "karma start --no-colors --single-run --log-level debug --reporters teamcity",
    "karma:watch": "karma start --autowatch",
    "mocha:watch": "mocha --compilers js:babel-core/register --require mocha-testHelper.js /UtopiaTargetFinder/Content/**/*-test.js --recursive -w --colors",
    "eslint": "eslint --ext .js UtopiaTargetFinder/Content/ && echo ESLint complete.",
    "mocha:coverage": "npm run eslint && nyc --silent --require babel-core/register npm run mocha && nyc report --reporter=lcov",
    "mocha:ci": "mocha --compilers js:babel-core/register --require mocha-testHelper.js --reporter mocha-teamcity-reporter ./UtopiaTargetFinder/Content/js/**/*-test.js --recursive",
    "mocha": "mocha --compilers js:babel-core/register --require mocha-testHelper.js /UtopiaTargetFinder/Content/**/*-test.js --recursive && npm run eslint",
    "clean": "rm -rf ./UtopiaTargetFinder/build",
    "purge": "rimraf ./node_modules/ packages && npm run clean && echo 'Purged node_modules, packages, and UtopiaTargetFinder builds'",
    "start": "set \"NODE_ENV=hot\" && node -- ./webpack-dev-server.js"
  },
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "6.0.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-react-jsx": "6.23.0",
    "babel-polyfill": "^6.6.1",
    "babel-preset-airbnb": "^2.2.3",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-preset-stage-1": "^6.5.0",
    "constant-mirror": "^1.1.1",
    "css-loader": "^0.26.2",
    "enzyme": "^2.0.0",
    "escope": "3.6.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-config-exchange-solutions": "^6.0.0",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "expect": "^1.14.0",
    "file-loader": "^0.10.1",
    "imports-loader": "^0.7.1",
    "jsdom": "^9.11.0",
    "json-loader": "^0.5.4",
    "karma": "^1.5.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-eslint": "^2.0.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.2",
    "less-loader": "^2.2.2",
    "mocha": "^3.2.0",
    "mocha-teamcity-reporter": "^1.1.1",
    "node-sass": "^4.5.0",
    "react-addons-test-utils": "^15.0.0",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.5.2",
    "sass-loader": "^6.0.3",
    "sinon": "^1.17.3",
    "sinon-as-promised": "^4.0.0",
    "style-loader": "^0.13.0",
    "superagent": "^3.5.0",
    "superagent-no-cache": "^0.1.1",
    "url-loader": "^0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.6.1",
    "bootstrap": "^3.3.6",
    "history": "^3.0.0",
    "less": "^2.6.0",
    "lodash": "4.17.4",
    "moment": "^2.11.2",
    "prop-types": "^15.5.8",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.0.0",
    "react-addons-shallow-compare": "^15.0.0",
    "react-bootstrap": "^0.30.0",
    "react-dom": "^15.0.0",
    "react-redux": "^5.0.3",
    "react-redux-toastr": "^5.0.0",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.2.1",
    "redux-thunk": "^2.2.0",
    "superagent": "3.5.0",
    "to-fast-properties": "^1.0.2",
    "uuid": "^3.0.1"
  }
}
